// RAPHAEL AZAMBUJA SILVA MACEDO
#include <stdio.h>

int main()
{
    int M,N;
    int i;
    long double somaA=1, somaB=1;



    while(scanf("%d %d" ,&M,&N) != EOF)
    {
        for (i=1;i<=M;i++)
        {
            somaA=somaA*i;
        }
        for (i=1;i<=N;i++)
        {
            somaB=somaB*i;
        }

        printf("%.0Lf\n" ,somaA+somaB);
        somaA = somaB = 1;
    }

    return 0;
}
